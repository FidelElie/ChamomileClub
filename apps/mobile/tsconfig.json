{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/assets": ["./assets/index.ts"],
      "@/assets/*": ["./assets/*"],
      "@/components/*": ["./components/*"],
      "@/library/*": ["./library/*"]
